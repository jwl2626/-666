using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace To_types1
{
    class Program
    {
            static void Main(string[] args)
            {
                //要求强制double 转换int 
                int i;
                double j = 2564.233;
                i = (int)j;
                Console.WriteLine("Line 1 - c 的值是:{0}", i);
                Console.ReadKey();

                int a = 75;
                float f = 53.005f;
                double d = 2345.7652;
                bool b = true;

                Console.WriteLine(a.ToString());
                Console.WriteLine(f.ToString());
                Console.WriteLine(d.ToString());
                Console.WriteLine(b.ToString());
                Console.ReadKey();


            }
        }
    }


